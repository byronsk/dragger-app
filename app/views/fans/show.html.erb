
<%= @fan.name %>

<%= link_to "Create your team", new_fan_team_path(@fan) %>

 <% if @fan.team %>
    <%= @fan.team.team_name %>
     <% @fan.team.queens.each do |queen| %>
    <%= queen.name %>
    <%= image_tag(queen.image_url, height: '150', width: '150') %>
    <%= queen.quote %>
  <% end %>
 <% end %>
